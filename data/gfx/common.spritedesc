sprite_sheet "common" 1024 x 1024 margin 1

light_star "effect/light-star.xcf.bz2"
rounded_star "effect/rounded-star.xcf.bz2"
balloon_img "balloon/balloon.xcf.bz2"
zeppelin_img "zeppelin/zeppelin.xcf.bz2"
zeppelin_mirror "zeppelin/zeppelin-mirror.xcf.bz2"
balloon_speech "speech/balloon.xcf.bz2"
switching_img "switching/switching.xcf.bz2"
switching_warning_img "switching/warning.xcf.bz2"

"light star" autosize * 0.125 with light_star
  "gradient"
  "disc"
  "circle"
  "circle fill"
  ;

"light star dark" autosize * 0.125 with light_star
  "dark"
  "inside"
  ;

"rounded star bright" autosize * 0.125 with rounded_star
  "bright border"
  "inside"
  ;

/*----------------------------------------------------------------------------*/
"balloon piece 1" autosize * 0.5 with balloon_img
  "balloon 2 - 1"
  [hollow] "explode color"
  ;

"balloon piece 2" autosize * 0.5 with balloon_img
