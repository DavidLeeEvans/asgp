/*  */
sprite_sheet "balloon-1" 512 x 512 margin 1

balloon "balloon.xcf.bz2"

[ bleed ] "spike" autosize * 0.5 with balloon
  glob "spike*"
  ;

[ bleed ] "vertical border" autosize * 0.5 with balloon
  glob "vertical border*"
  ;

[ bleed ] "horizontal border" autosize * 0.5 with balloon
  glob "horizontal border*"
  ;

[ bleed ] "corner" autosize * 0.5 with balloon
  glob "corner*"
  ;
