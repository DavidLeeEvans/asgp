/*  */
sprite_sheet "wall-fill-1" 256 x 256 margin 0

img "wall-fill.xcf.bz2"

"wall-fill" image_size * 0.5 with img

  glob "common *"

  glob "plank 1*"
  glob "plank 2*"
  glob "plank 3*"
  glob "plank 4*"
  "h plank 1 right"
  "h plank 2 left"

  "plank 5 lights"
  "plank 5"
  "h plank 1 lights 1"
  "h plank 1 left 1"

  "h plank 2 lights 1"
  "h plank 2 right 1"
  ;

