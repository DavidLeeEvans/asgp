/* The size of the resulting image. */
sprite_sheet "zeppelin" 256 x 256

/* the source images and their ratio in the result */
img "zeppelin.xcf.bz2"

"FUN" autosize * 0.5 with img
  "FUN"
  "blank screen"
  ;

