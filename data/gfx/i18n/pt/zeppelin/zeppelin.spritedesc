  "blank screen"
  ;

"UM" autosize * 0.5 with img
  "UM"
  "blank screen"
  ;

"NOME" autosize * 0.5 with img
  "NOME"
  "blank screen"
  ;

"MELHOR" autosize * 0.5 with img
  "MELHOR"
  "blank screen"
  ;


"blank screen" autosize * 0.5 with img
  "blank screen"
  ;

